Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"checkEq", "[", 
    RowBox[{"mat_", ",", "vec_", ",", "val_"}], "]"}], ":=", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"mat", ".", "vec"}], "-", 
     RowBox[{"val", "*", "vec"}]}], ")"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"checkEigensystem", "[", "mat_", "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"checkEq", "[", 
      RowBox[{"mat", ",", 
       RowBox[{
        RowBox[{"eigVecs", "[", "mat", "]"}], "[", 
        RowBox[{"[", "i", "]"}], "]"}], ",", 
       RowBox[{
        RowBox[{"eigVals", "[", "mat", "]"}], "[", 
        RowBox[{"[", "i", "]"}], "]"}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Length", "[", "mat", "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"validSystem", "[", "system_", "]"}], ":=", 
   RowBox[{"AllTrue", "[", 
    RowBox[{
     RowBox[{"Flatten", "[", "system", "]"}], ",", 
     RowBox[{
      RowBox[{"#", "\[Equal]", "0"}], "&"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"checkValid", "[", "system_", "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"validSystem", "[", "system", "]"}], ",", 
