Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"printMatrix", "[", "mat_", "]"}], ":=", 
   RowBox[{"Print", "[", 
    RowBox[{"mat", "//", "MatrixForm"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"eigenvalues", "[", "mat_", "]"}], ":=", 
   RowBox[{"Sort", "[", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Re", "[", 
       RowBox[{"Values", "@", 
        RowBox[{
         RowBox[{"Solve", "[", 
          RowBox[{
           RowBox[{"Det", "[", 
            RowBox[{"mat", "-", 
             RowBox[{"\[Lambda]", "*", 
              RowBox[{"unitMat", "[", 
               RowBox[{"Length", "[", "mat", "]"}], "]"}]}]}], "]"}], 
           "\[Equal]", "0"}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"i", ",", "1"}], "]"}], "]"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", 
        RowBox[{"Length", "[", "mat", "]"}]}], "}"}]}], "]"}], "]"}]}], 
  ";"}]}], "Input",
